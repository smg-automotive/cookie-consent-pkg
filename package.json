{
  "name": "@smg-automotive/cookie-consent-pkg",
  "version": "0.0.0-development",
  "description": "A boilerplate package setup",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "version": "npm run build",
    "build": "rimraf dist && rollup -c",
    "lint": "eslint .",
    "format": "npm run lint -- --fix",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:debug": "node --inspect-brk --inspect=127.0.0.1:9229 ./node_modules/jest/bin/jest.js --runInBand"
  },
  "author": "SMG Automotive",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/smg-automotive/cookie-consent-pkg.git"
  },
  "homepage": "https://github.com/smg-automotive/cookie-consent-pkg#readme",
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "29.0.0",
    "@rollup/plugin-node-resolve": "16.0.3",
    "@rollup/plugin-typescript": "12.3.0",
    "@smg-automotive/eslint-config": "5.2.10",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.2",
    "@testing-library/user-event": "14.6.1",
    "@types/jest": "30.0.0",
    "@types/node": "24.10.9",
    "@types/react": "19.2.10",
    "@types/react-dom": "19.2.3",
    "jest": "30.2.0",
    "jest-environment-jsdom": "30.2.0",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "rimraf": "6.1.2",
    "rollup": "4.57.1",
    "rollup-plugin-dts": "6.3.0",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-preserve-directives": "0.4.0",
    "semantic-release": "25.0.2",
    "ts-jest": "29.4.6",
    "ts-node": "10.9.2",
    "tslib": "2.8.1",
    "typescript": "5.9.3"
  },
  "dependencies": {
    "globals": "16.5.0"
  }
}
