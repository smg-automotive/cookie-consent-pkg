{
  "name": "@smg-automotive/cookie-consent-pkg",
  "version": "0.0.0-development",
  "description": "A boilerplate package setup",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "version": "npm run build",
    "build": "rimraf dist && rollup -c",
    "lint": "eslint .",
    "format": "npm run lint -- --fix",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:debug": "node --inspect-brk --inspect=127.0.0.1:9229 ./node_modules/jest/bin/jest.js --runInBand"
  },
  "author": "SMG Automotive",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/smg-automotive/cookie-consent-pkg.git"
  },
  "homepage": "https://github.com/smg-automotive/cookie-consent-pkg#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "28.0.6",
    "@rollup/plugin-node-resolve": "16.0.1",
    "@rollup/plugin-typescript": "12.1.4",
    "@smg-automotive/eslint-config": "5.1.0",
    "@testing-library/jest-dom": "6.6.4",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/jest": "30.0.0",
    "@types/node": "22.17.0",
    "@types/react": "18.3.23",
    "jest": "30.0.5",
    "jest-environment-jsdom": "30.0.5",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "rimraf": "6.0.1",
    "rollup": "4.46.2",
    "rollup-plugin-dts": "6.2.1",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-preserve-directives": "0.4.0",
    "semantic-release": "24.2.7",
    "ts-jest": "29.4.1",
    "ts-node": "10.9.2",
    "typescript": "5.9.2"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "dependencies": {
    "globals": "16.3.0"
  }
}
